{
  "presets": [
    [
      "babel-preset-gatsby",
      {
        "targets": {
          "browsers": [">0.25%", "not dead"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "flow-runtime",
      {
        "assert": true,
        "annotate": true
      }
    ]
  ]
}
